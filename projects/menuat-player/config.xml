<?xml version="1.0" encoding="UTF-8"?><widget xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" id="com.menuat.player" version="0.1.1">
    <name>menuat-player</name>

    <description>Menuat native player.</description>

    <author>Author Name &lt;a@b.com></author>

    <preference name="KeyboardShrinksView" value="true"/>
